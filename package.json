    "gen:countries": "node scripts/generate-countries.js",
    "gen:languages": "node scripts/generate-languages.js",
    "gen:genres": "node scripts/generate-genres.js",
    "generate": "npm-run-all gen:*",
    "lang:extract": "NODE_ENV=development lingui extract --clean",
    "lang:compile": "lingui compile",
    "lang": "pnpm lang:extract && pnpm lang:compile",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:ci": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "postinstall": "patch-package"
